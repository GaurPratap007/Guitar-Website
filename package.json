{
  "name": "guitar-chords-site",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Lightweight, accessible, mobile-first chord/tabs site built with React, Vite, TypeScript, and Tailwind. Deployable to GitHub Pages.",
  "scripts": {
    "dev": "vite",
    "build": "npm run content:build && vite build",
    "preview": "vite preview",
    "content:build": "tsx scripts/buildContent.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@types/youtube": "^0.0.48",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "@vitejs/plugin-react": "^4.3.4"
  }
}

